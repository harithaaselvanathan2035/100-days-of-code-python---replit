char1=input("do you like 'hanging around'?:")
if char1 == "yes":
    print("oh!! you are spiderman")
else:
    print("then you are not a spiderman")

char2 = input("do you have a 'gravelly' voice?:")
if char2 == "yes":
    print("you are a korg")
else:
    print("aww, then you're not a korg")

char3 = input("dou you often feel 'marvellous'?:")
